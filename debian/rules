#!/usr/bin/make -f
%:
	dh $@ --with python3 --with=systemd


# Added because poweroff executable binary 
override_dh_strip:
	dh_strip -Xpoweroff

# Added because poweroff executable binary 
override_dh_shlibdeps:
	dh_shlibdeps --dpkg-shlibdeps-params=--ignore-missing-info
	
override_dh_installinit:
	dh_installinit --noscripts --name=pt-device-message
	dh_installinit --noscripts --name=pt-desktop-message-service
	dh_installinit --noscripts --name=pt-poweroff